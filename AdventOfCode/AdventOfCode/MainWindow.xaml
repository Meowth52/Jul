<Window x:Class="AdventOfCode.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="input" HorizontalAlignment="Left" Height="113" Margin="135,25,0,0" TextWrapping="Wrap" Text="input" VerticalAlignment="Top" Width="251" KeyUp="TextBox_KeyUp" AcceptsReturn="True"/>
        <TextBox x:Name="output" HorizontalAlignment="Left" Height="125" Margin="135,175,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="251"/>
        <Label x:Name="ValdDag" Content="Label" HorizontalAlignment="Left" Margin="248,-1,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Day5button" Content="Day 5" HorizontalAlignment="Left" Margin="0,28,0,0" VerticalAlignment="Top" Width="62" Click="onClick5"/>
        <Button x:Name="Day6button" Content="Day 6" HorizontalAlignment="Left" Margin="0,3,0,0" VerticalAlignment="Top" Width="62" Click="onClick6"/>

    </Grid>
</Window>    
